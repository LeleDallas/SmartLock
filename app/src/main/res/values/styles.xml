<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SCBSwitch" >
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/rosyBrown</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/boldGray</item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#42221f1f</item>
    </style>

    <style name="Clock">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/rosyBrown</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/boldGray</item>
    </style>
</resources>